% SPECIFICATION DCM 7 - "7x7"
%--------------------------------------------------------------------------

DCM.b = zeros(5,5,3); DCM.b(2,1,2) = 1; DCM.b(3,1,2) = 1; DCM.b(3,2,2) = 1; DCM.b(4,2,2) = 1; DCM.b(4,3,2) = 1; DCM.b(5,2,2) = 1; DCM.b(5,3,2) = 1; DCM.b(2,1,3) = 1; DCM.b(3,1,3) = 1; DCM.b(3,2,3) = 1; DCM.b(4,2,3) = 1; DCM.b(4,3,3) = 1; DCM.b(5,2,3) = 1; DCM.b(5,3,3) = 1;  

